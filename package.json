{
  "name": "playroom",
  "version": "0.0.0",
  "description": "Design with code, powered by your own component library",
  "bin": {
    "playroom": "bin/cli.js"
  },
  "scripts": {
    "prepare": "husky install",
    "start:basic": "./bin/cli.js start --config configs/basic/playroom.config.js",
    "build:basic": "./bin/cli.js build --config configs/basic/playroom.config.js",
    "serve:basic": "PORT=9000 serve configs/basic/dist",
    "start:mui": "./bin/cli.js start --config configs/mui/playroom.config.js",
    "build:mui": "./bin/cli.js build --config configs/mui/playroom.config.js",
    "serve:mui": "PORT=9000 serve configs/mui/dist",
    "build-and-serve:all": "pnpm build:all && pnpm serve:all",
    "lint": "eslint . && prettier --list-different '**/*.{js,md,ts,tsx}' && tsc --noEmit",
    "format": "prettier --write '**/*.{js,md,ts,tsx}'",
    "test": "jest",
    "deploy-preview": "surge -p ./configs/basic/dist"
  },
