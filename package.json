{
  "name": "playroom",
  "version": "0.0.0",
  "description": "Design with code, powered by your own component library",
  "bin": {
    "playroom": "bin/cli.js"
  },
  "scripts": {
    "prepare": "husky install",
    "start:basic": "./bin/cli.js start --config configs/basic/playroom.config.js",
    "build:basic": "./bin/cli.js build --config configs/basic/playroom.config.js",
    "serve:basic": "PORT=9000 serve configs/basic/dist",
    "start:all": "concurrently 'npm:start:basic' 'npm:start:themed'",
    "build-and-serve:all": "pnpm build:all && pnpm serve:all",
    "lint": "eslint . && prettier --list-different '**/*.{js,md,ts,tsx}' && tsc --noEmit",
    "format": "prettier --write '**/*.{js,md,ts,tsx}'",
    "test": "jest",
    "deploy-preview": "surge -p ./configs/basic/dist"
  },
  "dependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/standalone": "^7.13.11",
    "@codemirror/autocomplete": "^6.3.4",
    "@codemirror/commands": "^6.1.2",
    "@codemirror/lang-javascript": "^6.1.1",
    "@codemirror/language": "^6.3.1",
    "@codemirror/lint": "^6.1.0",
    "@codemirror/search": "^6.2.3",
    "@codemirror/state": "^6.1.4",
    "@codemirror/view": "^6.6.0",
    "@lezer/common": "^1.0.2",
    "@lezer/highlight": "^1.1.3",
    "@soda/friendly-errors-webpack-plugin": "^1.8.0",
    "@types/base64-url": "^2.2.0",
    "@types/codemirror": "^5.60.5",
    "@types/dedent": "^0.7.0",
    "@types/lodash": "^4.14.168",
    "@types/lz-string": "^1.3.34",
    "@types/prettier": "^2.2.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@vanilla-extract/babel-plugin": "^1.2.0",
    "@vanilla-extract/css": "^1.7.0",
    "@vanilla-extract/css-utils": "^0.1.2",
